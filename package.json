{
    "name": "boilerplate",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "dev": "webpack-dev-server --config ./configs/webpack.config.js --open --hot --env.NODE_ENV=develop",
        "build": "webpack --config ./configs/webpack.config.js --mode production --progress --hide-modules --env.NODE_ENV=production",
        "test:styles": "stylelint \"src/**/*.{vue,scss}\""
    },
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@babel/preset-env": "^7.9.6",
        "autoprefixer": "^9.8.0",
        "babel-eslint": "^8.2.5",
        "babel-jest": "^21.0.2",
        "babel-loader": "^8.1.0",
        "copy-webpack-plugin": "^6.0.3",
        "css-loader": "^3.5.3",
        "cssnano": "^4.1.10",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.1.0",
        "eslint-loader": "^2.2.1",
        "eslint-plugin-cypress": "^2.10.3",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jest": "^23.8.2",
        "eslint-plugin-vue": "^6.2.2",
        "eslint-plugin-vue-a11y": "0.0.31",
        "file-loader": "^6.0.0",
        "html-webpack-plugin": "^4.3.0",
        "imagemin-webpack-plugin": "^2.4.2",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^7.0.0",
        "postcss-loader": "^3.0.0",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.2.1",
        "stylelint": "^13.3.3",
        "stylelint-config-airbnb": "0.0.0",
        "stylelint-config-prettier": "^4.0.0",
        "stylelint-config-recommended-scss": "^4.2.0",
        "stylelint-config-standard": "^18.2.0",
        "stylelint-order": "^4.0.0",
        "stylelint-scss": "^3.17.1",
        "stylelint-webpack-plugin": "^2.0.0",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "url-loader": "^4.1.0",
        "vue-loader": "^15.9.2",
        "vue-style-loader": "^4.1.2",
        "vue-template-compiler": "^2.6.11",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.11.0"
    },
    "dependencies": {
        "vue": "^2.6.11",
        "vue-router": "^3.2.0"
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {},
            "cssnano": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not Explorer > 0",
        "IE >= 11",
        "not ExplorerMobile > 0",
        "not BlackBerry > 0",
        "not OperaMini all",
        "not OperaMobile > 0"
    ],
    "prettier": {
        "arrowParens": "always",
        "semi": true,
        "printWidth": 80,
        "singleQuote": true,
        "trailingComma": "es5",
        "arrowParens:": "always",
        "tabWidth": 4
    }
}
